<?xml version="1.0" encoding="utf-8" ?>
<Window
    x:Class="App1.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:App1"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:t="using:TagListOrder"
    mc:Ignorable="d">
    <StackPanel Orientation="Vertical">
        <StackPanel.Resources>
            <x:String x:Key="name1">(C73)[T2 ART WORKS (Tony)] きてるよ! 竹内くんっ (バンブーブレード) [无修正]</x:String>
        </StackPanel.Resources>

        <local:UserControl2 Source="{StaticResource name1}" SplitFunc="a_SplitFunc" />

        <StackPanel x:Name="dontload"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            x:Load="False"
            Orientation="Vertical">

            <ListView
                x:Name="aa"
                AllowDrop="True"
                CanDragItems="True"
                CanReorderItems="True" />
            <t:UserControl1
                x:Name="a"
                SourceString="{StaticResource name1}"
                SplitFunc="a_SplitFunc" />

            <t:UserControl1
                x:Name="fdaf"
                SourceString="{StaticResource name1}"
                SplitFunc="a_SplitFunc" />
        </StackPanel>

    </StackPanel>

</Window>
